@model NativaGaragem.Models.Vaga

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Vaga</legend>

        @Html.HiddenFor(model => model.IDVaga)

        <div class="editor-label">
            @Html.LabelFor(model => model.IDCarro, "Carro")
        </div>
        <div class="editor-field">
            @Html.DropDownList("IDCarro", String.Empty)
            @Html.ValidationMessageFor(model => model.IDCarro)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.IDGaragem, "Garagem")
        </div>
        <div class="editor-field">
            @Html.DropDownList("IDGaragem", String.Empty)
            @Html.ValidationMessageFor(model => model.IDGaragem)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
